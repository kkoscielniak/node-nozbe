{
  "name": "node-nozbe",
  "version": "1.2.3",
  "description": "Nozbe REST API wrapper for Node.js",
  "homepage": "https://github.com/kkoscielniak/node-nozbe",
  "main": "lib/node-nozbe.js",
  "module": "lib/node-nozbe.mjs",
  "jsnext:main": "lib/node-nozbe.mjs",
  "keywords": [
    "nozbe",
    "node",
    "api"
  ],
  "scripts": {
    "docs": "npx jsdoc-to-markdown -f src/oauth.js src/task.js src/comment.js --template docs/API.hbs > docs/API.md",
    "build": "NODE_PATH=src rollup -c",
    "precommit": "npm run test",
    "prebuild": "rimraf ./lib",
    "prepublish": "NODE_PATH=src npm run build",
    "test": "jest .",
    "watch": "NODE_PATH=src rollup -c -w"
  },
  "author": "Krystian Ko≈õcielniak",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babelrc-rollup": "^3.0.0",
    "eslint": "4.13.1",
    "eslint-config-brainhub": "1.6.3",
    "eslint-watch": "3.1.3",
    "jest": "^22.4.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "qs": "^6.5.1",
    "querystring": "^0.2.0",
    "request": "^2.85.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  }
}
